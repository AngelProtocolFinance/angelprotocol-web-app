{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": false,
  "importOrder": [
    "^assets.*",
    "^pages.*",
    "^services.*",
    "^providers.*",
    "^components.*",
    "^store.*",
    "^contracts.*",
    "^hooks.*",
    "^helpers.*",
    "^constants.*",
    "^[../]",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true
}
