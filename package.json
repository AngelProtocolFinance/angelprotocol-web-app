{
  "name": "angelprotocol-web-app",
  "sideEffects": false,
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && rsbuild build",
    "start": "rsbuild dev",
    "test": "vitest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-staged": "lint-staged --config lint-staged.cjs",
    "preview": "rsbuild preview",
    "format": "yarn run prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": "18.16.1"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "6.0.3",
    "@cosmjs/encoding": "0.31.1",
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/bignumber": "5.7.0",
    "@headlessui/react": "1.7.17",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "3.3.1",
    "@keplr-wallet/proto-types": "0.12.23",
    "@keplr-wallet/wc-qrcode-modal": "0.12.23",
    "@reduxjs/toolkit": "1.9.7",
    "@sentry/react": "7.81.0",
    "@terra-money/terra.js": "3.1.10",
    "@terra-money/wallet-provider": "3.11.2",
    "@types/react-helmet": "6.1.9",
    "@walletconnect/modal": "2.6.2",
    "@walletconnect/sign-client": "2.10.5",
    "aws-amplify": "6.0.5",
    "cropperjs": "1.6.0",
    "decimal.js": "10.4.3",
    "is-mobile": "4.0.0",
    "quill": "1.3.7",
    "react": "18.2.0",
    "react-csv": "2.2.2",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.48.2",
    "react-icons": "4.12.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.21.1",
    "type-fest": "4.8.3",
    "yup": "1.3.0"
  },
  "devDependencies": {
    "@keplr-wallet/types": "0.12.23",
    "@rsbuild/core": "0.2.8",
    "@rsbuild/plugin-node-polyfill": "0.2.8",
    "@rsbuild/plugin-react": "0.2.8",
    "@tailwindcss/container-queries": "0.1.1",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/node": "18.18.13",
    "@types/quill": "2.0.14",
    "@types/react": "18.2.39",
    "@types/react-csv": "1.1.9",
    "@types/react-dom": "18.2.17",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@walletconnect/types": "2.10.5",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "5.1.1",
    "husky": "8.0.3",
    "jsdom": "23.0.1",
    "lint-staged": "15.0.2",
    "postcss-import": "15.1.0",
    "prettier": "3.0.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.4.0",
    "tsc-files": "1.1.4",
    "typescript": "<5.2.0",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.1.0"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    }
  }
}
