{
  "name": "angelprotocol-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "GENERATE_SOURCEMAP=false react-app-rewired start",
    "test": "react-app-rewired test --colors",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "serve-build": "yarn build && yarn dlx serve -s build -p 4200",
    "format": "yarn run prettier --write 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": "16.18.0"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.29.0",
    "@cosmjs/encoding": "^0.29.0",
    "@cosmjs/proto-signing": "^0.29.0",
    "@cosmjs/stargate": "^0.29.0",
    "@headlessui/react": "^1.7.4",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.8",
    "@keplr-wallet/types": "^0.11.3",
    "@keplr-wallet/wc-client": "^0.11.31",
    "@keplr-wallet/wc-qrcode-modal": "^0.11.31",
    "@reduxjs/toolkit": "^1.9.0",
    "@terra-money/terra.js": "^3.1.7",
    "@terra-money/wallet-provider": "^3.11.1",
    "@types/react-helmet": "^6.1.6",
    "@walletconnect/client": "^1.8.0",
    "@walletconnect/qrcode-modal": "^1.8.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "compress.js": "^1.2.2",
    "cropperjs": "^1.5.12",
    "decimal.js": "^10.3.1",
    "ethereum-multicall": "^2.15.0",
    "ethers": "^5.7.1",
    "is-mobile": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.42.1",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.2.0",
    "react-scripts": "5.0.1",
    "web3.storage": "^4.3.0",
    "yup": "^0.32.9"
  },
  "resolutions": {
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@peculiar/webcrypto": "^1.4.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.2.1",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/compress.js": "^1.1.1",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.18.3",
    "@types/quill": "^2.0.9",
    "@types/react": "^18.0.0",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-test-renderer": "18.0.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "source-map-explorer": "^2.5.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "<4.8.0",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "web-vitals": "^3.0.2"
  }
}
