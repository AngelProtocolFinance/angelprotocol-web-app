{
  "name": "angelprotocol-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --colors",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "serve-build": "yarn build && yarn dlx serve -s build -p 4200",
    "format": "yarn run prettier --write 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "16.13.2"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.28.9",
    "@cosmjs/encoding": "^0.28.10",
    "@cosmjs/proto-signing": "^0.28.10",
    "@cosmjs/stargate": "^0.28.10",
    "@draft-js-plugins/counter": "^4.1.2",
    "@draft-js-plugins/editor": "^4.1.3",
    "@fleekhq/fleek-storage-js": "^1.0.21",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.7",
    "@keplr-wallet/types": "^0.10.5",
    "@reduxjs/toolkit": "^1.7.1",
    "@terra-money/terra.js": "^3.1.5",
    "@terra-money/wallet-provider": "^3.9.4",
    "compress.js": "^1.2.2",
    "cropperjs": "^1.5.12",
    "decimal.js": "^10.3.1",
    "draft-js": "^0.11.7",
    "ethereum-multicall": "^2.12.0",
    "ethers": "^5.5.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "quill": "^1.3.7",
    "react": "^17.0.2",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.4.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.2.0",
    "react-select": "^5.4.0",
    "react-select-country-list": "^2.2.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@peculiar/webcrypto": "^1.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/compress.js": "^1.1.1",
    "@types/draft-js": "^0.11.9",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^18.6.5",
    "@types/quill": "^2.0.9",
    "@types/react": "^17.0.39",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-select-country-list": "^2.2.0",
    "@types/react-test-renderer": "17.0.2",
    "autoprefixer": "^10.0.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.8",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "17.0.2",
    "source-map-explorer": "^2.5.2",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "util": "^0.12.4",
    "web-vitals": "^1.0.1"
  }
}
