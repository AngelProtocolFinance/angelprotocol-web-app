{
  "name": "angelprotocol-web-app",
  "sideEffects": false,
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --colors",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-staged": "lint-staged --config lint-staged.js",
    "serve-build": "yarn build && yarn dlx serve -s build -p 4200",
    "format": "yarn run prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "jest": {
    "moduleNameMapper": {
      "@aws-amplify/ui-react/styles.css": "@aws-amplify/ui-react/dist/styles.css"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": "18.16.1"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "6.0.3",
    "@cosmjs/encoding": "0.31.1",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@headlessui/react": "^1.7.4",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "3.3.1",
    "@keplr-wallet/proto-types": "0.12.23",
    "@keplr-wallet/wc-qrcode-modal": "0.12.23",
    "@reduxjs/toolkit": "^1.9.0",
    "@sentry/react": "7.81.0",
    "@terra-money/terra.js": "^3.1.7",
    "@terra-money/wallet-provider": "^3.11.1",
    "@types/react-helmet": "^6.1.6",
    "@walletconnect/modal": "2.6.2",
    "@walletconnect/sign-client": "2.11.0",
    "aws-amplify": "6.0.5",
    "cropperjs": "1.6.0",
    "decimal.js": "^10.3.1",
    "is-mobile": "^4.0.0",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "7.48.2",
    "react-icons": "4.12.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.2.0",
    "react-scripts": "5.0.1",
    "type-fest": "^3.9.0",
    "yup": "1.3.0"
  },
  "resolutions": {
    "tailwindcss": "3.3.0",
    "loader-utils@^2.0.0": "^2.0.4",
    "loader-utils@^3.2.0": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "7.23.4",
    "@keplr-wallet/types": "0.12.23",
    "@peculiar/webcrypto": "^1.4.0",
    "@tailwindcss/container-queries": "^0.1.0",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.16.1",
    "@types/quill": "^2.0.9",
    "@types/react": "^18.0.0",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-test-renderer": "18.0.0",
    "@walletconnect/types": "2.10.5",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "15.0.2",
    "node-stdlib-browser": "^1.2.0",
    "prettier": "3.0.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "3.3.0",
    "tsc-files": "^1.1.3",
    "typescript": "<4.8.0",
    "web-vitals": "3.4.0"
  }
}
