{
  "name": "ast",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ast/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "swc",
        "outputPath": "build/apps/ast",
        "index": "apps/ast/src/index.html",
        "baseHref": "/",
        "main": "apps/ast/src/index.tsx",
        "tsConfig": "apps/ast/tsconfig.app.json",
        "assets": [
          "apps/ast/src/favicon.png",
          {
            "glob": "**/*",
            "input": "libs/assets/src/assets",
            "output": "assets"
          }
        ],
        "styles": ["styles/index.css"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "configs/webpack.config.js",
        "postcssConfig": "apps/ast/postcss.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": false
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "progress": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "port": 4200,
        "buildTarget": "ast:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "ast:build:development"
        },
        "production": {
          "buildTarget": "ast:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ast/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/ast/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
